$(document).ready(function(){var t=$.trim($(".post-body").text()),e=[,"يناير","فبراير","مايو","أبريل","مارس","يونيو","يوليو","أغسطس","سبتمبر","أكتوبر","نوفمبر","ديسمبر"];if("[sitemap]"===t){var s=$(".post-body");$.ajax({url:"/feeds/posts/default?alt=json-in-script",type:"get",dataType:"jsonp",success:function(t){for(var a=[],n=0;n<t.feed.category.length;n++)a.push(t.feed.category[n].term);var a=a.join("/");s.html('<div class="btnt-sitemap"></div>'),$(".post-body .btnt-sitemap").text(a);var i=$(".btnt-sitemap").text().split("/"),r="";for(get=0;get<i.length;++get)r+="<span>"+i[get]+"</span>";$(".btnt-sitemap").html(r),$(".btnt-sitemap span").each(function(){var t=$(this),s=$(this).text();$.ajax({url:"/feeds/posts/default/-/"+s+"?alt=json-in-script",type:"get",dataType:"jsonp",success:function(a){for(var n="",i='<ul class="btnt-toc">',r=0;r<a.feed.entry.length;r++){for(var p=0;p<a.feed.entry[r].link.length;p++)if("alternate"==a.feed.entry[r].link[p].rel){n=a.feed.entry[r].link[p].href;break}var l=a.feed.entry[r].title.$t,d=a.feed.entry[r].published.$t,c=d.substring(0,4),o=d.substring(5,7),f=d.substring(8,10),v='<span class="day">'+f.replace(/^0+/,"")+'</span><span class="month">'+e[parseInt(o,10)]+' </span><span class="year">'+c+"</span>";i+='<li><div class="toc-date">'+v+'</div><div class="btnt-icon"></div><span class="btnt-post"><a href="'+n+'">'+l+"</a></span></li>"}i+="</ul>",t.replaceWith('<div class="btnt-toc-wrap"><div class="btnt-cat">'+s+"</div>"+i+"</div>")}})})}})}});
